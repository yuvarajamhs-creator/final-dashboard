{
  "name": "ads-dashboard-server",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "build": "cd ../client && npm install && cross-env CI=false DISABLE_ESLINT_PLUGIN=true npm run build",
    "start": "nodemon server.js",
    "seed": "node seed.js",
    "verify-supabase": "node verify-supabase.js",
